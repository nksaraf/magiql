{
  "name": "magiql",
  "version": "0.0.4-alpha.6",
  "description": "",
  "repository": "Nikhil Saraf",
  "license": "MIT",
  "author": "Nikhil Saraf <nsaraf98@gmail.com>",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "yarn build",
    "clean": "rm -rf dist",
    "build:node": "yarn tsup src/babel.ts src/cli.ts --format cjs",
    "build:common": "yarn tsup src/index.ts src/node.ts src/recoil-store.ts src/jotai-store.ts src/devtools.ts --format esm,cjs --legacy-output --dts --external react --external react-dom --external react-native",
    "watch:node": "yarn build:node --watch",
    "watch:common": "yarn build:common --watch",
    "build": "yarn clean && run-p build:*",
    "pub": "bump && yarn publish --access public --non-interactive",
    "watch": "yarn clean && run-p watch:*",
    "demo": "yarn next",
    "dev": "run-s build demo"
  },
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@types/prismjs": "^1.16.1",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "graphql": "^15.3.0",
    "next": "^9.5.2",
    "npm-run-all": "^4.1.5",
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-notion": "^0.8.1",
    "tsup": "^3.6.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/relay-runtime": "^10.0.3",
    "babel-plugin-relay": "^10.0.1",
    "cosmiconfig": "^7.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "jotai": "^0.6.2",
    "match-sorter": "^4.2.0",
    "prettier": "^2.1.1",
    "react-draggable": "^4.4.3",
    "react-query": "^2.23.0",
    "react-query-devtools": "^2.4.7",
    "recoil": "^0.0.10",
    "relay-compiler": "^10.0.1",
    "relay-runtime": "^10.0.1",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "peerDependencies": {
    "graphql": "*",
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "bin": {
    "magiql": "dist/cli.js"
  },
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "browser": "./dist/esm/index.js",
      "require": "./dist/index.js",
      "node": "./dist/index.js",
      "default": "./dist/esm/index.js"
    },
    "./babel": {
      "require": "./dist/babel.js",
      "node": "./dist/babel.js",
      "default": "./dist/babel.js"
    },
    "./devtools": {
      "import": "./dist/esm/devtools.js",
      "browser": "./dist/esm/devtools.js",
      "require": "./dist/devtools.js",
      "node": "./dist/devtools.js",
      "default": "./dist/esm/devtools.js"
    },
    "./node": {
      "import": "./dist/esm/node.js",
      "browser": "./dist/esm/node.js",
      "require": "./dist/node.js",
      "node": "./dist/node.js",
      "default": "./dist/esm/node.js"
    },
    "./recoil": {
      "import": "./dist/esm/recoil.js",
      "browser": "./dist/esm/recoil.js",
      "require": "./dist/recoil.js",
      "node": "./dist/index.js",
      "default": "./dist/esm/recoil.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "files": [
    "dist",
    "cli",
    "devtools",
    "babel",
    "client",
    "recoil",
    "node",
    "README.md"
  ]
}
