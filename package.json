{
  "name": "magiql",
  "version": "0.0.1-beta.8",
  "description": "",
  "repository": "Nikhil Saraf",
  "license": "MIT",
  "author": "Nikhil Saraf <nsaraf98@gmail.com>",
  "types": "dist/types",
  "files": [
    "dist",
    "babel",
    "codegen"
  ],
  "main": "dist/magiql.js",
  "module": "dist/magiql.esm.js",
  "bin": {
    "magiql": "dist/cli.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prepublishOnly": "yarn clean && yarn build && yarn types",
    "build": "npm-run-all --parallel build:*",
    "types": "tsc -p tsconfig.json",
    "pretest": "yarn build",
    "dev": "yarn test && chokidar \"src/**/*\" \"test/test.tsx\" -c \"yarn test\"",
    "build:main": "klap build --source src/index.ts --module dist/magiql.esm.js --main dist/magiql.js",
    "build:babel": "klap build --source src/babel/index.ts --main dist/babel.js  --no-minify",
    "build:codegen": "klap build --source src/codegen/index.ts --main dist/codegen.js --no-minify",
    "build:cli": "klap build --source src/cli.ts --main dist/cli.js --no-minify && yarn add:bang",
    "add:bang": "sed -i '' '1i\\\n#!/usr/bin/env node\n' \"./dist/cli.js\"",
    "add:bang2": "type \".temp.txt\" ./dist/cli.js > ./dist/cli.js",
    "start": "klap start",
    "watch": "klap watch",
    "test": "yarn babel test/test.tsx -o test/test.out.tsx --extensions=tsx --presets next/babel --plugins=./dist/babel.js"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.9.0",
    "@types/node": "^13.11.0",
    "chokidar-cli": "^2.1.0",
    "graphql": "^15.0.0",
    "klap": "^4.2.3",
    "next": "^9.3.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "typescript": "3.8.3"
  },
  "klap": {
    "example": "public/index.ts"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@graphql-codegen/cli": "^1.13.1",
    "@graphql-codegen/plugin-helpers": "1.13.1",
    "@graphql-codegen/typescript": "^1.13.1",
    "@graphql-codegen/visitor-plugin-common": "1.13.1",
    "@types/react-query": "gist:a00f0670c1d29e9490a1d1d856a3866d",
    "auto-bind": "^4.0.0",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "camel-case": "^4.1.1",
    "graphql-ast-types": "^1.0.2",
    "graphql-config": "^3.0.0-rc.1",
    "isomorphic-unfetch": "^3.0.0",
    "pascal-case": "^3.1.1",
    "react-query": "^1.2.1"
  },
  "peerDependencies": {
    "graphql": "^14.0.0",
    "react": "^16.8"
  }
}
