{
  "name": "magiql",
  "version": "0.0.2",
  "description": "",
  "repository": "Nikhil Saraf",
  "license": "MIT",
  "author": "Nikhil Saraf <nsaraf98@gmail.com>",
  "types": "dist/types",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "pkger build",
    "prepare": "node ./dist/cjs/cli.js",
    "pub": "bump && yarn publish --access public --non-interactive",
    "dev": "yarn test && chokidar \"src/**/*\" \"test/test.tsx\" -c \"yarn test\"",
    "test": "yarn babel test/test.tsx -o test/test.out.tsx --extensions=tsx --presets next/babel --plugins=./dist/cjs/babel.js",
    "test:codegen": "NODE_ENV=test node ./dist/cjs/cli.js",
    "postinstall": "node ./dist/cjs/cli.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/standalone": "^7.9.6",
    "@graphql-codegen/cli": "^1.13.5",
    "@graphql-codegen/plugin-helpers": "1.13.5",
    "@graphql-codegen/typescript": "^1.13.5",
    "@graphql-codegen/typescript-operations": "^1.13.5",
    "@graphql-codegen/visitor-plugin-common": "1.13.5",
    "@types/node": "^14.0.11",
    "@types/react": "^16.9.34",
    "auto-bind": "^4.0.0",
    "camel-case": "^4.1.1",
    "chokidar-cli": "^2.1.0",
    "dedent": "^0.7.0",
    "graphql": "^15.0.0",
    "graphql-ast-types": "^1.0.2",
    "graphql-config": "^3.0.1",
    "magic-components": "^0.0.2-beta.30",
    "next": "^9.3.6",
    "next-monaco-editor": "^0.0.1-beta.22",
    "pascal-case": "^3.1.1",
    "path": "^0.12.7",
    "pkger": "^0.0.2-next.16",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-query-devtools": "^1.1.5",
    "typescript": "^3.8.3",
    "use-debounce": "^3.4.2"
  },
  "pkger": {
    "target": "browser",
    "silent": "true",
    "entries": [
      "client",
      "hooks",
      "fetch",
      "devtools",
      {
        "name": "babel",
        "target": "node"
      },
      {
        "name": "codegen"
      },
      {
        "name": "cli",
        "target": "cli",
        "cmd": "magiql"
      }
    ]
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.0.0",
    "react-query": "^1.3.3"
  },
  "peerDependencies": {
    "graphql": "^15.0.0",
    "react": "^16.13"
  },
  "bin": {
    "magiql": "dist/cjs/cli.js"
  },
  "module": "dist/esm/magiql.js",
  "main": "dist/cjs/magiql.js",
  "browser": "dist/esm/magiql.js",
  "files": [
    "dist",
    "babel",
    "codegen",
    "client",
    "hooks",
    "dist/types",
    "fetch",
    "devtools"
  ]
}