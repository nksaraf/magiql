{
  "name": "magiql",
  "version": "0.0.3-next.8",
  "description": "",
  "repository": "Nikhil Saraf",
  "license": "MIT",
  "author": "Nikhil Saraf <nsaraf98@gmail.com>",
  "types": "dist/types",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "yarn tsup src/node.ts src/cli/cli.ts src/index.ts src/recoil.ts src/devtools.ts --format esm,cjs",
    "pub": "bump && yarn publish --access public --non-interactive",
    "dev": "yarn test && chokidar \"src/**/*\" \"test/test.tsx\" -c \"yarn test\"",
    "test": "yarn babel test/test.tsx -o test/test.out.tsx --extensions=tsx --presets next/babel --plugins=./dist/cjs/babel.js",
    "test:codegen": "NODE_ENV=test node ./dist/cjs/cli.js"
  },
  "devDependencies": {
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "graphql": "^15.3.0",
    "next": "^9.5.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tsup": "^3.6.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@types/relay-runtime": "^10.0.3",
    "babel-plugin-relay": "^10.0.1",
    "cosmiconfig": "^7.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "match-sorter": "^4.2.0",
    "prettier": "^2.1.1",
    "react-draggable": "^4.4.3",
    "react-query": "^2.10.0",
    "react-query-devtools": "^2.4.4",
    "recoil": "^0.0.10",
    "relay-compiler": "^10.0.1",
    "relay-config": "^10.0.1",
    "relay-runtime": "^10.0.1",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "peerDependencies": {
    "graphql": "*",
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "bin": {
    "magiql": "dist/cjs/cli.js"
  },
  "module": "dist/esm/magiql.js",
  "main": "dist/cjs/magiql.js",
  "browser": "dist/esm/magiql.js",
  "exports": {
    ".": {
      "import": "./dist/esm/magiql.js",
      "browser": "./dist/esm/magiql.js",
      "require": "./dist/cjs/production/magiql.js",
      "node": "./dist/cjs/production/magiql.js",
      "default": "./dist/esm/magiql.js"
    },
    "./client": {
      "import": "./dist/esm/client.js",
      "browser": "./dist/esm/client.js",
      "require": "./dist/cjs/production/client.js",
      "node": "./dist/cjs/production/client.js",
      "default": "./dist/esm/client.js"
    },
    "./hooks": {
      "import": "./dist/esm/hooks.js",
      "browser": "./dist/esm/hooks.js",
      "require": "./dist/cjs/production/hooks.js",
      "node": "./dist/cjs/production/hooks.js",
      "default": "./dist/esm/hooks.js"
    },
    "./fetch": {
      "import": "./dist/esm/fetch.js",
      "browser": "./dist/esm/fetch.js",
      "require": "./dist/cjs/production/fetch.js",
      "node": "./dist/cjs/production/fetch.js",
      "default": "./dist/esm/fetch.js"
    },
    "./devtools": {
      "import": "./dist/esm/devtools.js",
      "browser": "./dist/esm/devtools.js",
      "require": "./dist/cjs/production/devtools.js",
      "node": "./dist/cjs/production/devtools.js",
      "default": "./dist/esm/devtools.js"
    },
    "./babel": {
      "import": "./dist/esm/babel.js",
      "browser": "./dist/esm/babel.js",
      "require": "./dist/cjs/production/babel.js",
      "node": "./dist/cjs/production/babel.js",
      "default": "./dist/esm/babel.js"
    },
    "./codegen": {
      "import": "./dist/esm/codegen.js",
      "browser": "./dist/esm/codegen.js",
      "require": "./dist/cjs/production/codegen.js",
      "node": "./dist/cjs/production/codegen.js",
      "default": "./dist/esm/codegen.js"
    },
    "./cli": {
      "import": "./dist/esm/cli.js",
      "browser": "./dist/esm/cli.js",
      "require": "./dist/cjs/production/cli.js",
      "node": "./dist/cjs/production/cli.js",
      "default": "./dist/esm/cli.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "files": [
    "dist",
    "babel",
    "codegen",
    "client",
    "hooks",
    "dist/types",
    "fetch",
    "devtools",
    "README.md"
  ]
}
